{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/micro-ecomm/src",
  "monorepo": true,
  "root": "apps/micro-ecomm",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/micro-ecomm/tsconfig.app.json"
  },
  "projects": {
    "micro-ecomm": {
      "type": "application",
      "root": "apps/micro-ecomm",
      "entryFile": "main",
      "sourceRoot": "apps/micro-ecomm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/micro-ecomm/tsconfig.app.json"
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}